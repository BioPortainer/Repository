{
  "name": "19-HUMANN2",
  "script_path": "/GUI/conf/scripts/humann2.sh",
  "description": "HUMAnN2: The HMP Unified Metabolic Analysis Network 2",
  "working_directory": "/root/workdir/data",
  "parameters": [
    {
      "name": "Input",
      "param": "-i",
      "required": true,
      "description": "Input file of type {fastq,fastq.gz,fasta,fasta.gz,sam,bam,blastm8,genetable,biom}"
    },
    {
      "name": "Output dir",
      "param": "-o",
      "required": true,
      "description": "Directory to write output files"
    },
    {
      "name": "NT-data dir",
      "param": "--nucleotide-database",
      "description": "Directory containing the nucleotide database"
    },
    {
      "name": "PTN-data dir",
      "param": "--protein-database",
      "description": "Directory containing the protein database"
    },
    {
      "name": "Other parameters",
      "description": "Other parameters"
    },
    {
      "name": "Help",
      "param": "-h",
      "description": "Helpful help screen",
      "no_value": true,
      "default": false
    }
  ]
}
