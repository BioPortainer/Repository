{
  "name": "14.13-Fastx tool kit: FASTA-Q trimmer",
  "script_path": "/GUI/conf/scripts/D6_FASTA-Q_Trimmer.sh",
  "description": "Shortening reads in a FASTQ or FASTQ files (removing barcodes or noise)",
  "working_directory": "/root/workdir/data",
  "parameters": [
    {
      "name": "Input",
      "param": "-i",
      "required": true,
      "description": "FASTA/Q input file. default is STDIN"
    },
    {
      "name": "Output",
      "param": "-o",
      "required": true,
      "description": "FASTA/Q output file. default is STDOUT"
    },
    {
      "name": "1st to keep",
      "param": "-f",
      "required": true,
      "description": "First base to keep"
    },
        {
      "name": "Last to keep",
      "param": "-l",
      "required": true,
      "description": "Last base to keep"
    },
    {
      "name": "Other Parameters",
      "description": "Other Parameters"
    },
    {
      "name": "Help",
      "param": "-h",
      "no_value": true,
      "default": false,
      "description": "This helpful help screen"
    }
  ]
}
