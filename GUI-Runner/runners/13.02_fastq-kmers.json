{
  "name": "13.02-Fastq-tools: Fastq-kmers",
  "script_path": "conf/scripts/fastq-kmers.sh",
  "description": "Print kmer counts for the given kmer size. Output is in two tab-seperated columns for kmer and frequency",
  "working_directory": "./",
  "parameters": [
    {
      "name": "Input file",
      "type": "file_upload"
    },
    {
      "name": "Kmer size",
      "param": "-k",
      "type": "int",
      "description": "kmer size (default: 1)",
      "min": "1",
      "max": "16",
      "default": "1"
    },
    {
      "name": "Help",
      "param": "-h",
      "description": "Print help",
      "no_value": true,
      "default": false
    },
    {
      "name": "Version",
      "param": "-V",
      "description": "Output version information and exit",
      "no_value": true,
      "default": false
    }
  ]
}
